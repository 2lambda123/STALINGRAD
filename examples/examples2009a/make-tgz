#!/bin/tcsh -f
rm -rf stalingrad-examples2009
mkdir stalingrad-examples2009
cp -i {common,{saddle,particle}-FF}-{stalingrad.vlad,{adifor,tapenade}.f,fadbad++.cpp,{mlton,smlnj}.sml,ocaml.ml,ghc.hs,{bigloo,chicken,gambit,s2c,stalin}.sc,{ikarus,larceny,mitscheme,scmutils}.scm,{mzc,mzscheme}.ss} stalingrad-examples2009/.
cp -i {{saddle,particle}-{FR,RF,RR},probabilistic-{lambda-calculus,prolog}-{F,R}}-{stalingrad.vlad,{mlton,smlnj}.sml,ocaml.ml,{bigloo,chicken,gambit,s2c,stalin}.sc,{ikarus,larceny,mitscheme}.scm,{mzc,mzscheme}.ss} stalingrad-examples2009/.
cp -i probabilistic-{lambda-calculus,prolog}-F-scmutils.scm stalingrad-examples2009/.
cp -i backprop-Fs-{stalingrad.vlad,{adifor,tapenade}.f,adic.c,{adolc,cppad,fadbad++}.cpp,{mlton,smlnj}.sml,ocaml.ml,{bigloo,chicken,gambit,s2c,stalin}.sc,{ikarus,larceny,mitscheme,scmutils}.scm,{mzc,mzscheme}.ss} stalingrad-examples2009/.
cp -i backprop-Fv-{{adifor,tapenade}.f,adic.c,{adolc,fadbad++}.cpp} stalingrad-examples2009/.
cp -i backprop-R-{stalingrad.vlad,{tapenade}.f,{adolc,cppad,fadbad++}.cpp,{mlton,smlnj}.sml,ocaml.ml,{bigloo,chicken,gambit,s2c,stalin}.sc,{ikarus,larceny,mitscheme}.scm,{mzc,mzscheme}.ss} stalingrad-examples2009/.
cp -i {{saddle,particle}-{F,R}{F,R},probabilistic-{lambda-calculus,prolog}-{F,R},backprop-{Fs,R}}-stalingrad.c stalingrad-examples2009/.
cp -i backprop-Fs-adic-f.c stalingrad-examples2009/.
cp -i backprop-Fs-adic-f.h stalingrad-examples2009/.
cp -i backprop-Fs-adifor.adf stalingrad-examples2009/.
cp -i backprop-Fs-adifor.cmp stalingrad-examples2009/.
cp -i backprop-Fs-adifor-f.f stalingrad-examples2009/.
cp -i backprop-Fs-tapenade-f.f stalingrad-examples2009/.
cp -i backprop-Fv-adic-f.c stalingrad-examples2009/.
cp -i backprop-Fv-adic-f.h stalingrad-examples2009/.
cp -i backprop-Fv-adifor.adf stalingrad-examples2009/.
cp -i backprop-Fv-adifor.cmp stalingrad-examples2009/.
cp -i backprop-Fv-adifor-f.f stalingrad-examples2009/.
cp -i backprop-Fv-tapenade-f.f stalingrad-examples2009/.
cp -i backprop-R-tapenade-f.f stalingrad-examples2009/.
cp -i backprop-tapenade-DIFFSIZES.inc stalingrad-examples2009/.
cp -i common-adifor.inc stalingrad-examples2009/.
cp -i common-tapenade.inc stalingrad-examples2009/.
cp -i compile-and-run-examples stalingrad-examples2009/.
cp -i compile-examples stalingrad-examples2009/.
cp -i compile-stalingrad-examples stalingrad-examples2009/.
cp -i count-lines stalingrad-examples2009/.
cp -i diffscript stalingrad-examples2009/.
cp -i index.html stalingrad-examples2009/.
cp -i make-clean stalingrad-examples2009/.
cp -i make-enscript stalingrad-examples2009/.
cp -i make-tgz stalingrad-examples2009/.
cp -i make-web stalingrad-examples2009/.
cp -i particle-FF-adifor1.adf stalingrad-examples2009/.
cp -i particle-FF-adifor1.cmp stalingrad-examples2009/.
cp -i particle-FF-adifor1.f stalingrad-examples2009/.
cp -i particle-FF-adifor2.adf stalingrad-examples2009/.
cp -i particle-FF-adifor2.cmp stalingrad-examples2009/.
cp -i particle-FF-adifor2.f stalingrad-examples2009/.
cp -i particle-FF-adifor.inc stalingrad-examples2009/.
cp -i particle-FF-tapenade-DIFFSIZES1.inc stalingrad-examples2009/.
cp -i particle-FF-tapenade-DIFFSIZES2.inc stalingrad-examples2009/.
cp -i particle-FF-tapenade.inc stalingrad-examples2009/.
cp -i particle-FF-tapenade.sed stalingrad-examples2009/.
cp -i run-examples stalingrad-examples2009/.
cp -i run-examples1 stalingrad-examples2009/.
cp -i run.text stalingrad-examples2009/.
cp -i saddle-FF-adifor1.adf stalingrad-examples2009/.
cp -i saddle-FF-adifor1.cmp stalingrad-examples2009/.
cp -i saddle-FF-adifor1.f stalingrad-examples2009/.
cp -i saddle-FF-adifor2.adf stalingrad-examples2009/.
cp -i saddle-FF-adifor2.cmp stalingrad-examples2009/.
cp -i saddle-FF-adifor2.f stalingrad-examples2009/.
cp -i saddle-FF-adifor.inc stalingrad-examples2009/.
cp -i saddle-FF-tapenade-DIFFSIZES1.inc stalingrad-examples2009/.
cp -i saddle-FF-tapenade-DIFFSIZES2.inc stalingrad-examples2009/.
cp -i saddle-FF-tapenade.inc stalingrad-examples2009/.
cp -i saddle-FF-tapenade.sed stalingrad-examples2009/.
