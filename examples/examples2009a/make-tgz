cp -i tabulate.sc stalingrad-examples2009/.
cp -i talk.pdf stalingrad-examples2009/.
cp -i ~/bin/i686-Linux-2.6.26-2-686/stalingrad stalingrad-examples2009/stalingrad-i686
cp -i ~/bin/x86_64-Linux-2.6.26-2-amd64/stalingrad stalingrad-examples2009/stalingrad-amd64
tar zcf stalingrad-examples2009.tgz stalingrad-examples2009
rm -rf stalingrad-examples2009
