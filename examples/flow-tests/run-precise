#!/bin/tcsh

set EXAMPLE = $1
set NUM = $2

set OUTFILE=${EXAMPLE}-precise-`date +"%m%d%y"`-${NUM}.txt

set OPTS1 = "-no-warn -no-apply-multiply"
set OPTS2 = ""
set OPTS3 = "-expression-equality-using-structural"
set OPTS4 = ""

echo unbuff ">>" $OUTFILE time ./stalingrad -pp -flow-analysis\
$OPTS1 $OPTS2 $OPTS3 $OPTS4 \
${EXAMPLE}.vlad > $OUTFILE

unbuff >> $OUTFILE time ./stalingrad -pp -flow-analysis\
$OPTS1 $OPTS2 $OPTS3 $OPTS4\
${EXAMPLE}.vlad
