all: QobiScheme stalingrad

targets:
	@echo QobiScheme
	@echo stalingrad


QobiScheme:
	$(MAKE) -C QobiScheme all

stalingrad: QobiScheme
	$(MAKE) -C source all

test:
	rm -rf test-runs
	./tool/test-driver '(record-expectations! (fast-expectations))'
	$(MAKE) -C test-runs all
	./tool/test-failures

test-full:
	rm -rf test-runs
	./tool/test-driver '(record-expectations! (all-expectations))'
	$(MAKE) -C test-runs all
	./tool/test-failures

Makefile: Makefile.in
	./config.status

.PHONY: all targets QobiScheme stalingrad test test-full
