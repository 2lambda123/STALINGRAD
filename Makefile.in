all: QobiScheme stalingrad

targets:
	@echo QobiScheme
	@echo stalingrad


QobiScheme:
	$(MAKE) -C QobiScheme all

stalingrad: QobiScheme
	$(MAKE) -C source all

test:
	mit-scheme --compiler --heap 6000 --batch-mode --no-init-file --load test-driver.scm --eval '(parse-and-run-tests!)'

.PHONY: all targets QobiScheme stalingrad test
